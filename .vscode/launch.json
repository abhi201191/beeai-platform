    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "beeai_server.application:app",
                "--reload",
                "--port",
                "18333"
            ],
            "envFile": "${workspaceFolder}/apps/beeai-server/.env",
            "cwd": "${workspaceFolder}/apps/beeai-server/src",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}